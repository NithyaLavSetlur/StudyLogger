<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Database Table</title>
</head>
<body>
  <h1>Users List</h1>
  <table border="1">
    <thead>
      <tr>
        <th>carID</th>
        <th>make</th>
        <th>model</th>
      </tr>
    </thead>
    <tbody>
      <!-- Loop through the users and display each row -->
      <% users.forEach(cars => { %>
        <tr>
          <td><%= cars.carID %></td>
          <td><%= cars.make %></td>
          <td><%= cars.model %></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
