<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sessions Display</title>
    <link rel="stylesheet" href="public\styles.css">
</head>
<body>
    <h2>Study Sessions Display</h2>
    <table border="1">
        <thead>
            <tr>
                <th>Date</th>
                <th>Subject</th>
                <th>Start Time</th>
                <th>End Time</th>
                <th>Duration</th>
            </tr>
        </thead>
        <tbody>
            <% sessions.forEach(sessions => { %>
                <tr>
                    <td><%= sessions.session_date %></td>
                    <td><%= sessions.session_subject %></td>
                    <td><%= sessions.session_starttime %></td>
                    <td><%= sessions.session_endtime %></td>
                    <td><%= sessions.session_duration %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
